    </style>
    <style name="EmuMoviesProgress" parent="DialogProgressBar">
      <item name="android:layout_width">match_parent</item>
      <item name="android:layout_height">@dimen/spacing_tiny</item>
      <item name="android:layout_marginTop">@dimen/spacing_tiny</item>
      <item name="android:layout_marginLeft">0dp</item>
      <item name="android:layout_marginRight">0dp</item>
      <item name="android:progressDrawable">@drawable/loading_bar</item>
   </style>
    <style name="EmuMoviesProgressText">
      <item name="android:textColor">@color/pal_text1</item>
      <item name="android:textSize">@dimen/text_normal</item>
      <item name="android:layout_width">wrap_content</item>
      <item name="android:layout_height">wrap_content</item>
      <item name="android:singleLine">true</item>
      <item name="android:gravity">left</item>
      <item name="android:ellipsize">middle</item>
   </style>
    <style name="GamepadLabel">
        <item name="android:textSize">@dimen/text_small</item>
        <item name="android:textColor">@color/pal_text_hl</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
	</style>
    <style name="KeyButton">
		<item name="android:textColor">@color/pal_text1</item>
		<item name="android:textSize">@dimen/text_normal</item>
		<item name="android:background">@drawable/button_key</item>
		<item name="android:gravity">center</item>
		<item name="android:minHeight">0dp</item>
   </style>
